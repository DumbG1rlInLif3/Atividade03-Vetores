public class InicializacaoeAcesso {

    public static void main(String[] args) {

        // Criando um vetor de inteiros com 5 elementos
        int[] numeros = new int[5];

        // Atribuindo valores
        numeros[0] = 10;
        numeros[1] = 20;
        numeros[2] = 30;
        numeros[3] = 40;
        numeros[4] = 50;

        // Acessando e imprimindo valores
        System.out.println("Terceiro elemento: " + numeros[2]);

    }

}
